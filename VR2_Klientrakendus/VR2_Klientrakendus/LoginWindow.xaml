<Window x:Class="VR2_Klientrakendus.LoginWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:VR2_Klientrakendus"
        mc:Ignorable="d"
        Title="LoginWindow" Height="300" Width="300" ResizeMode="NoResize">
    <Grid>
        <TextBox GotFocus="TextUsername_GotFocus" x:Name="txtUserName" HorizontalAlignment="Left" Height="31" Margin="52,21,0,0" TextWrapping="Wrap" Text="Username" VerticalAlignment="Top" Width="191"/>
        <Button x:Name="btnLogIn" Content="Log in" HorizontalAlignment="Left" Margin="157,93,0,0" VerticalAlignment="Top" Width="86" Height="31" Click="btnLogIn_Click"/>
        <Button x:Name="btnRegister" Content="Register" HorizontalAlignment="Left" Margin="52,93,0,0" VerticalAlignment="Top" Width="88" Height="31" Click="btnRegister_Click"/>
        <PasswordBox  x:Name="TxtPassword" HorizontalAlignment="Left" Height="31" Margin="52,57,0,0" VerticalAlignment="Top" Width="191"/>

    </Grid>
</Window>
